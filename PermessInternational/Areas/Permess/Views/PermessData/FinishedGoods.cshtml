@model PermessInternational.Areas.Permess.ViewModels.GoodsTypeViewModels
<section class="content-header">
    <h1>
        Finished Goods
        <small>preview of finished goods</small>
    </h1>
    <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-dashboard"></i>Home</a></li>
    </ol>
</section>

<!-- Main content -->
<section class="content">
    <div class="row">
        <div class="col-md-12">
            <div class="box">
                <!-- /.box-header -->
                <div class="box-body">
                    <table class="table table-bordered table-responsive">
                        <thead>
                            <tr>
                                <th style="width: 10px">#</th>
                                <th>Date</th>
                                <th>Code</th>
                                <th>Product</th>
                                <th>Quantity</th>
                                <th>L.C</th>
                                <th>Supplier</th>
                                <th>Article</th>
                                <th>Alt. Article</th>
                                <th>Softness</th>
                                <th>Color</th>
                                <th>Width</th>
                                <th>Construction</th>
                                <th>Source</th>
                                <th>Description</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model.ResponseGoods)
                            {
                                <tr>
                                    <td>@item.Serial</td>
                                    <td>@item.ReceivedDate</td>
                                    <td>@item.PerformerCode</td>
                                    <td>@item.ProductName</td>
                                    <td>@item.Quantity</td>
                                    <td>@item.LCNumber</td>
                                    <td>@item.Supplier</td>
                                    <td>@item.Article</td>
                                    <td>@item.AltArticle</td>
                                    <td>@item.Softness</td>
                                    <td>@item.Color</td>
                                    <td>@item.Width</td>
                                    <td>@item.Construction</td>
                                    <td>@item.Source</td>
                                    <td>@item.Description</td>
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
                <!-- /.box-body -->
                <div class="box-footer clearfix">

                    <ul class="pagination pagination-sm no-margin pull-right">
                        <li><a href="#">&laquo;</a></li>
                        <li><a href="#">1</a></li>
                        <li><a href="#">2</a></li>
                        <li><a href="#">3</a></li>
                        <li><a href="#">&raquo;</a></li>
                    </ul>
                </div>
            </div>
            <!-- /.box -->
        </div>
        <!-- /.col -->

    </div>
</section>